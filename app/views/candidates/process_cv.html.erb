<div class="tab-content">
    <div id="artigos" class="tab-pane fade in active">
        <div class="panel panel-default">
            <div class="panel-heading text-center"><h4><strong>Artigos Identificados</strong></h4></div>
            <div class="panel-body form-inline">
                <input type="text" class="form-control" placeholder="Pesquisar">
            </div>
            <table class="table table-bordered" >
                <thead>
                    <tr>
                        <th>Título do Artigo</th>
                        <th>Periódico ou Revista</th>
                        <th>Categoria</th>
                        <th>Ano</th>
                    </tr>
                </thead>
                <tbody>
            <% @cv_handler.get_articles_found.each do |article| %>
            <tr>
                <td><%= article["DADOS_BASICOS_DO_ARTIGO"]["TITULO_DO_ARTIGO"] %></td>
                <td><%= article["DETALHAMENTO_DO_ARTIGO"]["TITULO_DO_PERIODICO_OU_REVISTA"] %></td>
                <td><%= article["qualis"] %></td>
                <td><%= article["DADOS_BASICOS_DO_ARTIGO"]["ANO_DO_ARTIGO"] %></td>
            </tr>
            <% end %>
            </tbody>
            </table>
            <div class="panel-footer">Total: <%= @cv_handler.get_articles_found_total_points%></div>
        </div>
        <div class="panel panel-default">
                <div class="panel-heading text-center"><h4><strong>Artigos Não Identificados</strong></h4></div>
                <div class="panel-body form-inline">
                    <input type="text" class="form-control" placeholder="Pesquisar">        
                </div>
        </div>
