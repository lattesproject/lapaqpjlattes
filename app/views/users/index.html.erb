<h1 align="center">Avaliação Lattes Usuários</h1>

<div align="center">
	<% @users.each do |user| %>

	<ul class="listing">

		<div class="row">

			<div class="well col-md-4 col-md-offset-4">

				<li class="event-title">

				<%= link_to user.username, user_path(user) %>

				</li>

				<li><small><%= pluralize(user.events.count, "evento") if user.events %></small></li>

			</div>

		</div>

	</ul>

	<% end %>
	<%= will_paginate %>
</div